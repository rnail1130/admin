{
  "name": "e-admin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm run serve",
    "dev": "npm run serve",
    "serve": "vue-cli-service serve --color --copy",
    "build": "vue-cli-service build --mode prod",
    "lint": "vue-cli-service lint",
    "lint:fix": "eslint --fix --ext .js,.vue src",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "dependencies": {
    "axios": "^0.19.0",
    "core-js": "^2.6.5",
    "element-ui": "^2.12.0",
    "js-base64": "^2.5.1",
    "md5": "^2.2.1",
    "moment": "^2.24.0",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "qs": "^6.9.0",
    "vue": "^2.6.10",
    "vue-router": "^3.0.7",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.10.0",
    "@vue/cli-plugin-eslint": "^3.10.0",
    "@vue/cli-service": "^3.10.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-vue": "^5.0.0",
    "gh-pages": "^2.1.1",
    "node-sass": "^7.0.0",
    "sass-loader": "^8.0.0",
    "style-resources-loader": "^1.2.1",
    "svg-sprite-loader": "^4.1.6",
    "vue-cli-plugin-style-resources-loader": "^0.1.3",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
